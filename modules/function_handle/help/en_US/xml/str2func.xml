	<language>en_US</language>
	<keyword>func2str</keyword>
	<short_description>Returns a string from function handle.</short_description>

	<syntax>
		<syntax_item>str = func2str(func_handle)</syntax_item>
	</syntax>
	
	<param_input>
		<param_input_item>
			<param_name>func_handle</param_name>
			<param_description>a function handle.</param_description>
		</param_input_item>
	</param_input>

	<param_output>

		<param_output_item>
			<param_name>str</param_name>
			<param_description>a string</param_description>
		</param_output_item>

	</param_output>

	<description>
<p><b>str = func2str(function_handle)</b> returns a string <b>str</b> that holds the name of the function to which the function handle is associated.</p>
</description>


	<used_function></used_function>
	<bibliography></bibliography>

<examples>

	<example_item>
		<example_item_type>nelson</example_item_type>
		<example_item_description></example_item_description>
		<example_item_data><![CDATA[fh = str2func('cos')
str = func2str(fh)]]>
		</example_item_data>
	
	</example_item>

</examples>

