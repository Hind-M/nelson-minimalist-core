	<language>en_US</language>
	<keyword>h5create</keyword>
	<short_description>Creates a dataset.</short_description>

	<syntax>
		<syntax_item>h5create(filename, datasetname, size, Name1, Value1, ..., NameN, ValueN)</syntax_item>
	</syntax>
	
	<param_input>
		<param_input_item>
			<param_name>filename</param_name>
			<param_description>a string: hdf5 filename.</param_description>
		</param_input_item>
		<param_input_item>
			<param_name>datasetname</param_name>
			<param_description>a string: name of the data set.</param_description>
		</param_input_item>
		<param_input_item>
			<param_name>size</param_name>
			<param_description>a row vector specifying the extents of the dataset.</param_description>
		</param_input_item>
		<param_input_item>
			<param_name>Name1, Value1, ..., NameN, ValueN</param_name>
			<param_description>Name-Value Pair Arguments.</param_description>
		</param_input_item>

	</param_input>

	<description>
		<p><b>h5create</b> creates a dataset and specify its extent dimensions, datatype and chunk size.</p>
</description>


	<used_function></used_function>
	<bibliography></bibliography>

<examples>

	<example_item>
		<example_item_type>nelson</example_item_type>
		<example_item_description></example_item_description>
		<example_item_data><![CDATA[h5create([tempdir(), '/myfile.h5'],'/myDataset1',[10 20]);]]>
		</example_item_data>
	</example_item>

</examples>

