A = [8     1     6;3     5     7;4     9     2];
B = [15; 15; 15];
R1 = A \ B;
R2 = mldivide(A, B);
assert_isequal(R1, R2);
REF = [ 1.0000;1.0000;1.0000];
assert_isapprox(R1, REF, 1e-4);
