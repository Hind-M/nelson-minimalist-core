//=============================================================================
R = 1 + 1;
REF = 2;
assert_isequal(R, REF);
//=============================================================================
R = ones(3, 0) + 1;
REF = zeros(3, 0);
assert_isequal(R, REF);
//=============================================================================
R = ones(3, 0) + ones(3, 0);
REF = zeros(3, 0);
assert_isequal(R, REF);
//=============================================================================
R = ones(3, 3) + ones(3, 3);
REF = ones(3, 3) * 2;
assert_isequal(R, REF);
//=============================================================================
R = 1 + zeros(3, 3, 3);
REF = ones(3, 3, 3);
assert_isequal(R, REF);
//=============================================================================
R = zeros(3, 3, 3) + 1;
REF = ones(3, 3, 3);
assert_isequal(R, REF);
//=============================================================================
R = ones(3, 3, 3) + ones(3, 3, 3);
REF = ones(3, 3, 3) + 1;
assert_isequal(R, REF);
//=============================================================================
R = 3 + i;
REF = complex(3, 1);
assert_isequal(R, REF);
//=============================================================================
